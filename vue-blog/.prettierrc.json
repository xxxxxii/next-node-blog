{
    "prettier/prettier": 0,
    "printWidth": 100,
    "useTabs": false,
    "tabWidth": 4,
    "tabSize": 4,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "quotes": true,
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "rangeStart": 0,
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf"
}
